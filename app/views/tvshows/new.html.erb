<h1>Add a Tv Show</h1>

<%= form_for @tvshows do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :year %>
    <%= f.text_field :year %>
  </p>
  <p>
    <%= f.label :image %>
    <%= f.url_field :image %>
  </p>
  <p>
    <%= f.label :plot %>
    <%= f.text_field :plot %>
  </p>

  <%= f.submit %>
<% end %>
